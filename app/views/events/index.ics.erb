BEGIN:VCALENDAR
VERSION:2.0
CALSCALE:GREGORIAN
<% @events.each do |event| %>
BEGIN:VEVENT
UID:manabiya:event:<%= event.id %>
DTSTAMP:<%= event.created_at.strftime('%Y%m%dT%H%M%SZ') %>
DTSTART:<%= event.start_at.strftime('%Y%m%dT%H%M%S') %>
SUMMARY:<%= event.title %>
DESCRIPTION:
LOCATION:<%= event.venue %>
URL:<%= event_url(event) %>
END:VEVENT
<% end %>
END:VCALENDAR
